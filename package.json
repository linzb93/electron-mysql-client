{
  "name": "mysql-client-lite",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "轻量mysql客户端",
  "author": "linzb93",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:app": "electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@vitejs/plugin-vue": "^5.0.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "sass": "^1.71.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "element-plus": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "vue-router": "^4.3.0"
  },
  "build": {
    "appId": "com.linzb93.mysql",
    "productName": "Mysql Client",
    "mac": {
      "icon": "public/logo.icns"
    }
  }
}
